{
    "name": "konnec/vue-eloquent-api",
    "type": "package",
    "description": "Vue Eloquent API",
    "keywords": [
        "Vue",
        "Laravel",
        "Api",
        "Eloquent",
        "Model",
        "Validation",
        "Broadcast",
        "Vuelidate",
        "Echo"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "hammerstone/fast-paginate": "^0.1",
        "illuminate/http": ">=7.30",
        "illuminate/support": ">=7.30"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "driftingly/rector-laravel": "^0.26.2",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.10",
        "nunomaduro/larastan": "^2.6",
        "orchestra/testbench": "^8.13",
        "pestphp/pest": "^2.24",
        "pestphp/pest-plugin-faker": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.2",
        "phpunit/phpunit": "^10.4",
        "rector/rector": "^0.18.5",
        "spatie/laravel-ignition": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Konnec\\VueEloquentApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Konnec\\Examples\\": "examples/",
            "Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "scripts": {
        "larastan": [
            "./vendor/bin/phpstan analyse --memory-limit 1024M"
        ],
        "larastan:baseline": [
            "./vendor/bin/phpstan analyse --generate-baseline --memory-limit 1024M"
        ],
        "pint": [
            "./vendor/bin/pint"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "rector-dry-run": [
            "vendor/bin/rector --dry-run"
        ],
        "rector": [
            "vendor/bin/rector"
        ],
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "@build",
            "@php vendor/bin/testbench serve"
        ],
        "lint": [
            "@php vendor/bin/pint",
            "@php vendor/bin/phpstan analyse"
        ],
        "test": [
            "@php vendor/bin/pest"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Konnec\\VueEloquentApi\\Providers\\EloquentApiAppServiceProvider",
                "Konnec\\VueEloquentApi\\Providers\\EloquentApiRouteServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
