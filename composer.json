{
    "name": "konnec/vue-eloquent-api",
    "type": "package",
    "description": "Vue Eloquent API",
    "keywords": [
        "Vue",
        "Laravel",
        "Api",
        "Eloquent",
        "Model",
        "Validation",
        "Broadcast",
        "Vuelidate",
        "Echo"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "hammerstone/fast-paginate": "^0.1.12"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.6",
        "pestphp/pest-plugin-faker": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.2",
        "phpunit/phpunit": "^10.3",
        "spatie/laravel-ignition": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Konnec\\VueEloquentApi\\": "src/",
            "Konnec\\VueEloquentApi\\Database\\Factories\\": "src/database/factories/",
            "Konnec\\VueEloquentApi\\Database\\Seeders\\": "src/database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "larastan": [
            "./vendor/bin/phpstan analyse --memory-limit 1024M"
        ],
        "larastan:baseline": [
            "./vendor/bin/phpstan analyse --generate-baseline --memory-limit 1024M"
        ],
        "pint": [
            "./vendor/bin/pint"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ],
        "rector-dry-run": [
            "vendor/bin/rector --dry-run"
        ],
        "rector": [
            "vendor/bin/rector"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
